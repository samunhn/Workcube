//Code for Workcube

#include <PRIZM.h>      //link up the library
PRIZM prizm;            //declare object(reference to prism)

void setup(){
  prizm.PrizmBegin();
}
void loop(){
  prizm.setMotorPower(1,100);   //100% speed
  delay(2000);
  prizm.setMotorPower(1,125);   //125 means break
  delay(2000);
}
